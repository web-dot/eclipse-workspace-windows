	*/
	
	
	
	public static void main(String[] args) {
		
		String a = "ate";	
		String b = "tea";  	
		
		
		
		
		
		char[] c1 = a.toCharArray();
		char[] c2 = b.toCharArray();
		
		System.out.println(c1);
		System.out.println(c2);
		
		StringBuilder sb = new StringBuilder();
		for(int i = 0; i < c1.length; i++) {
			if(c1[i] < c1[i+1]) {
				sb.append(c1);
			}
		}

	}

}
